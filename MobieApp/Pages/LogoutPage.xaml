<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobieApp.Pages.LogoutPage"
             Title="Logout">
    <Grid Padding="20">
        <VerticalStackLayout VerticalOptions="Center" Spacing="20">
            <Label FontSize="20" HorizontalOptions="Center" TextColor="OrangeRed" Text="Yakin Keluar ? "></Label>
            <Button Command="{Binding LogoutCommand}" Text="Keluar" HeightRequest="50"></Button>
        </VerticalStackLayout>
    </Grid>
</ContentPage>